.btnmenu
	position				absolute
	font-weight				bold
	color 					#777
	-webkit-transition 		all 0.3s
	-moz-transition 		all 0.3s

.btnmenu:hover
	cursor					pointer
	font-size 				40px
	color 					#333
	-webkit-transition 		all 0.5s
	-moz-transition 		all 0.5s

.topLeft
	top 					0
	left 					0
	
.topRight
	top						0
	right 					0
	
.bottomLeft
	bottom 					0
	left 					0

.bottomRight
	bottom 					0
	right 					0